{
    "venvPath": ".",
    "venv": "venv",
    "pythonVersion": "3.13",
    "pythonPlatform": "Darwin",
    "include": [
        "backend",
        "scripts",
        "alembic"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules",
        "venv",
        ".venv"
    ],
    "typeCheckingMode": "off",
    "reportMissingImports": "none",
    "reportMissingTypeStubs": "none",
    "executionEnvironments": [
        {
            "root": "backend",
            "python": "../venv/bin/python",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "../venv/lib/python3.13/site-packages"
            ]
        },
        {
            "root": "scripts",
            "python": "../venv/bin/python",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "../backend",
                "../venv/lib/python3.13/site-packages"
            ]
        },
        {
            "root": ".",
            "python": "./venv/bin/python",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "venv/lib/python3.13/site-packages"
            ]
        }
    ]
}
